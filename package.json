{
  "name": "gatsby-remark-github",
  "version": "0.0.0-semantic-release",
  "description": "GatsbyJS plugin to inline GitHub source code into markdown using remark-github-plugin",
  "keywords": [
    "gatsby",
    "gatsbyjs",
    "remark",
    "github",
    "markdown",
    "unified"
  ],
  "author": "Huy Nguyen <me@huynguyen.io> (https://www.huynguyen.io/)",
  "main": "index.js",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huy-nguyen/gatsby-remark-github.git"
  },
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "commit": "commit",
    "publish": "semantic-release"
  },
  "dependencies": {
    "remark-github-plugin": "1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "6.1.3",
    "@commitlint/config-conventional": "6.1.3",
    "@commitlint/prompt-cli": "6.1.3",
    "husky": "0.14.3",
    "semantic-release": "15.1.7"
  }
}
